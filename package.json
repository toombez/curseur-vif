{
  "name": "curseur-vif",
  "version": "0.0.0",
  "description": "Vue.js library for creating custom cursors",
  "license": "MIT",
  "keywords": [
    "vue",
    "cursor",
    "color-manipulation"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@vueuse/core": "^11.1.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.0",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "pnpm@8.6.12",
  "homepage": "https://github.com/toombez/curseur-vif",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toombez/curseur-vif.git"
  },
  "bugs": {
    "url": "https://github.com/toombez/curseur-vif/issues"
  },
  "author": {
    "name": "Timur Tokaev",
    "url": "https://github.com/toombez"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./composables": {
      "import": "./dist/composables.js",
      "require": "./dist/composables.cjs",
      "types": "./dist/composables.d.ts"
    },
    "./directives": {
      "import": "./dist/directives.js",
      "require": "./dist/directives.cjs",
      "types": "./dist/directives.d.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "types": "./dist/plugin.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts"
}